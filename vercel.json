{
  "version": 2,
  "builds": [{ "src": "index.js", "use": "@now/node"}],
  "routes": [{ "src": "/(.*)", "dest": "/index.js" }],
  "env": {
    "MONGO_URI": "@clo-mongo-uri",
    "SECRET": "@clo-secret",
    "FIREBASE_API_KEY": "@clo-fire-key",
    "FIREBASE_AUTH_DOMAIN": "@clo-fire-auth",
    "FIREBASE_DATABASE_URL": "@clo-fire-db",
    "FIREBASE_PROJECT_ID": "@clo-fire-id",
    "FIREBASE_STORAGE_BUCKET": "@clo-fire-store",
    "FIREBASE_MESSAGING_SENDER_ID": "@clo-fire-message",
    "FIREBASE_APP_ID": "@clo-fire-app",
    "S3_SECRET_ACCESS_KEY": "@clo-s3-key",
    "S3_ACCESS_KEY_ID": "@clo-s3-id",
    "S3_BUCKET" : "@clo-s3-buc",
    "CORS": "*"
  }
}